	doi = {10.1787/5a376df4-en},
	keywords = {Bilateral Investment Treaties, Divestment, Double Taxation Agreements, International Investment, Multinational Firms, Preferential Trade Agreements},
	file = {Full Text PDF:C\:\\Users\\adamc\\Zotero\\storage\\G5FU2BIH\\Borga et al. - 2020 - Drivers of divestment decisions of multinational e.pdf:application/pdf},
