	author = {Schmitt, Matthias and Gottwalt, Albin and Winkler, Jakob and Tobie, Thomas and Schlick, Georg and Stahl, Karsten and Tetzlaff, Ulrich and Schilp, Johannes and Reinhart, Gunther},
	month = jun,
	year = {2021},
	note = {Number: 6
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {additive manufacturing, multi-material, PBF-LB/M, 16MnCr5, case-hardening steel, gears, in situ alloying},
	pages = {896},
	file = {Full Text PDF:C\:\\Users\\adamc\\Zotero\\storage\\Y5MLL6XV\\Schmitt et al. - 2021 - Carbon Particle In-Situ Alloying of the Case-Harde.pdf:application/pdf},
